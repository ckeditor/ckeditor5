    "editor",
    "html",
    "contentEditable",
    "editing",
    "operational transformation",
    "ot",
    "collaboration",
    "collaborative",
    "real-time",
    "framework"
  ],
  "devDependencies": {
    "@babel/parser": "^7.23.3",
    "@ckeditor/ckeditor5-dev-build-tools": "^53.1.0",
    "@ckeditor/ckeditor5-dev-bump-year": "^53.1.0",
    "@ckeditor/ckeditor5-dev-changelog": "^53.1.0",
    "@ckeditor/ckeditor5-dev-ci": "^53.1.0",
    "@ckeditor/ckeditor5-dev-dependency-checker": "^53.1.0",
    "@ckeditor/ckeditor5-dev-docs": "^53.1.0",
    "@ckeditor/ckeditor5-dev-release-tools": "^53.1.0",
    "@ckeditor/ckeditor5-dev-stale-bot": "^53.1.0",
    "@ckeditor/ckeditor5-dev-tests": "^53.1.0",
    "@ckeditor/ckeditor5-dev-web-crawler": "^53.1.0",
    "@ckeditor/ckeditor5-inspector": "^5.0.0",
    "@ckeditor/ckeditor5-mermaid": "ckeditor/ckeditor5-mermaid#v0.0.5",
    "@ckeditor/ckeditor5-react": "^9.5.0",
    "@inquirer/prompts": "^7.5.0",
    "@listr2/prompt-adapter-inquirer": "^2.0.16",
    "@ocular-d/vale-bin": "^2.29.1",
    "@webspellchecker/wproofreader-ckeditor5": "^3.0.0",
    "acorn": "^8.11.3",
    "assert": "^2.0.0",
    "chalk": "^5.0.0",
    "coveralls": "^3.1.0",
    "date-fns": "^4.0.0",
