    "@ckeditor/ckeditor5-heading": "workspace:*",
    "@ckeditor/ckeditor5-horizontal-line": "workspace:*",
    "@ckeditor/ckeditor5-image": "workspace:*",
    "@ckeditor/ckeditor5-indent": "workspace:*",
    "@ckeditor/ckeditor5-link": "workspace:*",
    "@ckeditor/ckeditor5-list": "workspace:*",
    "@ckeditor/ckeditor5-media-embed": "workspace:*",
    "@ckeditor/ckeditor5-mention": "workspace:*",
    "@ckeditor/ckeditor5-paragraph": "workspace:*",
    "@ckeditor/ckeditor5-source-editing": "workspace:*",
    "@ckeditor/ckeditor5-special-characters": "workspace:*",
    "@ckeditor/ckeditor5-table": "workspace:*",
    "@ckeditor/ckeditor5-typing": "workspace:*",
    "@ckeditor/ckeditor5-undo": "workspace:*",
    "@ckeditor/ckeditor5-code-block": "workspace:*",
